<h1>Queries</h1>
<br />
<pre>
select movie_name from movie
where rating_number in ( select rating_number from movie where rating_number >= 9);
</pre>
<table border="1">
    <tr>

        <th>Name</th>


    </tr>
    <%for(var i = 0; i < q_result1.length;i++){ %>
    <tr>

        <td><%= q_result1[i].movie_name %></td>

    </tr>

    <% } %>
</table>
<pre>
    select movie_name from movie
    left join movie_rating on movie.rating_number = movie_rating.rating_number
    order by movie_rating_id;
</pre>
<table border="1">
    <tr>

        <th>Name</th>


    </tr>
    <%for(var i = 0; i < q_result2.length;i++){ %>
    <tr>

        <td><%= q_result2[i].movie_name %></td>

    </tr>

    <% } %>
</table>
<pre>
    select movie_name from movie m
    left join movie_rating mr on m.rating_number = mr.rating_number
    group by m.rating_number
    having  m.rating_number >= avg(m.rating_number) ;
</pre>
<table border="1">
    <tr>

        <th>Name</th>


    </tr>
    <%for(var i = 0; i < q_result2.length;i++){ %>
    <tr>

        <td><%= q_result2[i].movie_name %></td>

    </tr>

    <% } %>
</table>
</body>
</html>